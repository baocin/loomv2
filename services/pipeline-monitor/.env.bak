# Pipeline Monitor Frontend Environment Variables

# API endpoint for the backend service
# For LAN access, use your machine's IP address instead of localhost
# Example: http://192.168.1.100:8082
VITE_API_URL=http://10.0.0.88:8082

# WebSocket endpoint (usually same as API_URL but with ws:// protocol)
VITE_WS_URL=ws://localhost:8082/ws

# Kafka Test Producer endpoint
VITE_TEST_PRODUCER_URL=http://localhost:8008

# Refresh intervals (in milliseconds)
VITE_METRICS_REFRESH_INTERVAL=5000
VITE_HEALTH_CHECK_INTERVAL=10000

# Cache settings
VITE_MAX_CACHED_MESSAGES=100
